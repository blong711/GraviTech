    },
    {
      "type": "header",
      "content": "t:sections.main-cart.settings.section_padding_heading.content"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.main-cart.settings.padding_top.label",
      "default": 0
