<div class="flat-spacing-2">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-xl-4 col-sm-8">
                {% for block in section.blocks %}
                    {% case block.type %}
                        {% when 'free_shipping_progress' %}
                            <div class="tf-cart-head text-center" {{ block.shopify_attributes }}>
                                {% assign cart_total = cart.total_price %}
                                {% assign free_shipping_threshold = settings.free_shipping_threshold | times: 100 %}
                                {% assign remaining_for_free_shipping = free_shipping_threshold | minus: cart_total %}
                                {% assign progress_percentage = cart_total | times: 100.0 | divided_by: free_shipping_threshold | at_most: 100 %}
                                
                                {% if cart_total >= free_shipping_threshold %}
                                    <p class="text-xl-3 title text-dark-4">{{ block.settings.qualified_message | default: "You've qualified for" }} <span class="fw-medium">{{ block.settings.free_shipping_text | default: "Free Shipping!" }}</span></p>
                                {% else %}
                                    <p class="text-xl-3 title text-dark-4">{{ block.settings.spend_message | default: "Spend" }} <span class="fw-medium">${{ remaining_for_free_shipping | round: 2 }}</span> {{ block.settings.more_message | default: "more to get" }}
                                        <span class="fw-medium">{{ block.settings.free_shipping_text | default: "Free Shipping" }}</span>
                                    </p>
                                {% endif %}
                                
                                <div class="progress-sold tf-progress-ship">
                                    <div class="value" style="width: {{ progress_percentage }}%;" data-progress="{{ progress_percentage }}">
                                        <i class="icon icon-{{ block.settings.progress_icon | default: 'car' }}"></i>
                                    </div>
                                </div>
                            </div>
                    {% endcase %}
                {% endfor %}
            </div>
        </div>
    </div>
</div>

<!-- Cart Section -->
<div class="flat-spacing-2 pt-0">
    <div class="container">
        <div class="row">
            <div class="col-xl-8">
                <div class="tf-page-cart-main">
                    {% if cart.item_count > 0 %}
                        {% for block in section.blocks %}
                            {% case block.type %}
                                {% when 'cart_items_table' %}
                                    <form action="/cart" method="post" class="form-cart" {{ block.shopify_attributes }}>
                                        <table class="table-page-cart">
                                            <thead>
                                                <tr>
                                                    <th>{{ block.settings.product_column_header | default: 'Product' }}</th>
                                                    <th>{{ block.settings.price_column_header | default: 'Price' }}</th>
                                                    <th>{{ block.settings.quantity_column_header | default: 'Quantity' }}</th>
                                                    <th>{{ block.settings.total_column_header | default: 'Total' }}</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for item in cart.items %}
                                                    <tr class="tf-cart-item file-delete" data-item-key="{{ item.key }}">
                                                        <td class="tf-cart-item_product">
                                                            <a href="{{ item.url }}" class="img-box">
                                                                <img src="{{ item.image | img_url: '150x150', crop: 'center' }}" alt="{{ item.title | escape }}" width="150" height="150">
                                                            </a>
                                                            <div class="cart-info">
                                                                <a href="{{ item.url }}" class="name text-md link fw-medium">{{ item.product.title }}</a>
                                                                {% if item.variant.title != 'Default Title' %}
                                                                    <div class="variants">{{ item.variant.title }}</div>
                                                                {% endif %}
                                                                <span class="remove-cart link remove" data-item-key="{{ item.key }}">{{ block.settings.remove_text | default: 'Remove' }}</span>
                                                            </div>
                                                        </td>
                                                        <td class="tf-cart-item_price text-center" data-cart-title="Price">
                                                            {% if item.original_price != item.final_price %}
                                                                <span class="cart-price price-on-sale text-md fw-medium">{{ item.final_price | money }}</span>
                                                                <span class="cart-price original-price text-sm text-muted"><s>{{ item.original_price | money }}</s></span>
                                                            {% else %}
                                                                <span class="cart-price text-md fw-medium">{{ item.final_price | money }}</span>
                                                            {% endif %}
                                                        </td>
                                                        <td class="tf-cart-item_quantity" data-cart-title="Quantity">
                                                            <div class="wg-quantity">
                                                                <span class="btn-quantity btn-decrease" data-item-key="{{ item.key }}">-</span>
                                                                <input class="quantity-product" type="number" name="updates[]" value="{{ item.quantity }}" min="1" data-item-key="{{ item.key }}">
                                                                <span class="btn-quantity btn-increase" data-item-key="{{ item.key }}">+</span>
                                                            </div>
                                                        </td>
                                                        <td class="tf-cart-item_total text-center" data-cart-title="Total">
                                                            <div class="cart-total total-price text-md fw-medium">{{ item.final_line_price | money }}</div>
                                                        </td>
                                                    </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </form>
                                
                                {% when 'gift_wrap' %}
                                    {% if block.settings.show_gift_wrap %}
                                        <div class="check-gift" {{ block.shopify_attributes }}>
                                            <input type="checkbox" class="tf-check" id="checkGift" name="attributes[Gift Wrap]">
                                            <label for="checkGift" class="label text-dark-4">
                                                {%- if settings.gift_wrap_product != blank -%}
                                                    {%- assign gift_wrap_product = all_products[settings.gift_wrap_product] -%}
                                                    {%- if gift_wrap_product != blank -%}
                                                        {{ block.settings.label_text | default: 'Add gift wrap. Only' }} <span class="fw-medium">{% if settings.currency_code_enabled %}{{ gift_wrap_product.price | money }} {{ shop.currency }}{% else %}{{ gift_wrap_product.price | money }}{% endif %}.</span> {{ block.settings.optional_text | default: '(You can choose or not)' }}
                                                        <input type="hidden" name="gift_wrap_product_id" value="{{ gift_wrap_product.selected_or_first_available_variant.id }}">
                                                    {%- else -%}
                                                        {{ block.settings.label_text | default: 'Add gift wrap. Only' }} <span class="fw-medium">$10.00.</span> {{ block.settings.optional_text | default: '(You can choose or not)' }}
                                                    {%- endif -%}
                                                {%- else -%}
                                                    {{ block.settings.label_text | default: 'Add gift wrap. Only' }} <span class="fw-medium">$10.00.</span> {{ block.settings.optional_text | default: '(You can choose or not)' }}
                                                {%- endif -%}
                                            </label>
                                        </div>
                                    {% endif %}
                                
                                {% when 'discount_code' %}
                                    <div class="box-ip-discount" {{ block.shopify_attributes }}>
                                        <input type="text" placeholder="{{ block.settings.placeholder_text | default: 'Discount code' }}" name="discount">
                                        <button type="button" class="tf-btn radius-6 btn-out-line-dark-2">{{ block.settings.button_text | default: 'Apply' }}</button>
                                    </div>
                                
                                {% when 'cart_note' %}
                                    <div class="cart-note" {{ block.shopify_attributes }}>
                                        <label for="note" class="text-md fw-medium">{{ block.settings.label_text | default: 'Special instructions for seller' }}</label>
                                        <textarea id="note" name="note" placeholder="{{ block.settings.placeholder_text }}">{{ cart.note }}</textarea>
                                    </div>
                                

                                {% when 'features_slider' %}
                                    <div class="fl-iconbox wow fadeInUp" {{ block.shopify_attributes }}>
                                        <div dir="ltr" class="swiper tf-swiper sw-auto" data-swiper='{
                                            "slidesPerView": 1,
                                            "spaceBetween": 12,
                                            "speed": 800,
                                            "observer": true,
                                            "observeParents": true,
                                            "slidesPerGroup": 1,
                                            "pagination": { "el": ".sw-pagination-iconbox", "clickable": true },
                                            "breakpoints": {
                                                "575": { "slidesPerView": 2, "spaceBetween": 12, "slidesPerGroup": 2}, 
                                                "768": { "slidesPerView": 3, "spaceBetween": 24, "slidesPerGroup": 3},
                                                "1200": { "slidesPerView": "auto", "spaceBetween": 24}
                                            }
                                        }'>
                                            <div class="swiper-wrapper">
                                                {% if block.settings.features != blank %}
                                                    {% assign features = block.settings.features | split: ',' %}
                                                    {% for feature in features %}
                                                        {% assign feature_parts = feature | split: '|' %}
                                                        {% assign icon = feature_parts[0] | strip %}
                                                        {% assign title = feature_parts[1] | strip %}
                                                        <div class="swiper-slide">
                                                            <div class="tf-icon-box justify-content-center justify-content-sm-start style-3">
                                                                <div class="box-icon">
                                                                    <i class="icon icon-{{ icon | default: 'shipping' }}"></i>
                                                                </div>
                                                                <div class="content">
                                                                    <div class="title text-uppercase">{{ title | default: 'Free Shipping' }}</div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    {% endfor %}
                                                {% endif %}
                                            </div>
                                        </div>
                                        <div class="d-flex d-xl-none sw-dot-default sw-pagination-iconbox justify-content-center"></div>
                                    </div>
                            {% endcase %}
                        {% endfor %}
                    {% else %}
                        {% for block in section.blocks %}
                            {% case block.type %}
                                {% when 'empty_cart' %}
                                    <div class="empty-cart text-center" {{ block.shopify_attributes }}>
                                        <h3>{{ block.settings.empty_cart_title | default: 'Your cart is empty' }}</h3>
                                        <p>{{ block.settings.empty_cart_message | default: "Looks like you haven't added any items to your cart yet." }}</p>
                                        <a href="{{ block.settings.continue_shopping_url | default: '/collections/all' }}" class="tf-btn btn-dark2 animate-btn">{{ block.settings.continue_shopping_text | default: 'Continue Shopping' }}</a>
                                    </div>
                            {% endcase %}
                        {% endfor %}
                    {% endif %}
                </div>
            </div>
            
            <div class="col-xl-4">
                <div class="tf-page-cart-sidebar">
                    {% if cart.item_count > 0 %}
                        {% for block in section.blocks %}
                            {% case block.type %}
                                {% when 'shipping_estimator' %}
                                    <form class="cart-box shipping-cart-box" {{ block.shopify_attributes }}>
                                        <div class="text-lg title fw-medium">{{ block.settings.title | default: 'Shipping estimates' }}</div>
                                        <fieldset class="field">
                                            <label for="country" class="text-sm">{{ block.settings.country_label | default: 'Country' }}</label>
                                            <input type="text" id="country" placeholder="United State">
                                        </fieldset>
                                        <fieldset class="field">
                                            <label for="state" class="text-sm">{{ block.settings.state_label | default: 'State/Province' }}</label>
                                            <input type="text" id="state" placeholder="{{ block.settings.state_placeholder | default: 'State/Province' }}">
                                        </fieldset>
                                        <fieldset class="field">
                                            <label for="code" class="text-sm">{{ block.settings.zipcode_label | default: 'Zipcode' }}</label>
                                            <input type="text" id="code" placeholder="{{ block.settings.zipcode_placeholder | default: '41000' }}">
                                        </fieldset>
                                        <button type="button" class="tf-btn btn-dark2 animate-btn w-100">{{ block.settings.button_text | default: 'Estimate' }}</button>
                                    </form>
                                
                                {% when 'cart_summary' %}
                                    <form action="/cart" method="post" class="cart-box checkout-cart-box" {{ block.shopify_attributes }}>
                                        <div class="cart-head">
                                            <div class="total-discount text-xl fw-medium">
                                                <span>{{ block.settings.subtotal_label | default: 'Subtotal:' }}</span>
                                                <span class="total">{{ cart.total_price | money }}</span>
                                            </div>
                                            
                                            {% if cart.total_discounts > 0 %}
                                                <div class="discounts text-sm text-success">
                                                    <span>{{ block.settings.discounts_label | default: 'Discounts:' }}</span>
                                                    <span>-{{ cart.total_discounts | money }}</span>
                                                </div>
                                            {% endif %}
                                            
                                            <p class="text-sm text-dark-4">{{ block.settings.tax_message | default: 'Taxes and shipping calculated at checkout' }}</p>
                                        </div>
                                        
                                        {% if block.settings.show_terms_checkbox %}
                                            <div class="check-agree">
                                                <input type="checkbox" class="tf-check" id="check-agree" required>
                                                <label for="check-agree" class="label text-dark-4">{{ block.settings.terms_text | default: 'I agree with' }} <a href="{{ block.settings.terms_url | default: '/pages/terms-and-conditions' }}" class="text-dark-4 fw-medium text-underline link">{{ block.settings.terms_link_text | default: 'terms and conditions' }}</a></label>
                                            </div>
                                        {% endif %}
                                        
                                        <div class="checkout-btn">
                                            <button type="submit" name="checkout" class="tf-btn btn-dark2 animate-btn w-100">{{ block.settings.checkout_button_text | default: 'Checkout' }}</button>
                                        </div>
                                        
                                        {% if block.settings.show_payment_icons %}
                                            <div class="cart-imgtrust">
                                                <p class="text-center text-sm text-dark-1">{{ block.settings.payment_methods_title | default: 'We accept' }}</p>
                                                <div class="cart-list-social">
                                                    {% if block.settings.payment_svg_list != blank %}
                                                        {% assign payment_icons = block.settings.payment_svg_list | split: ',' %}
                                                        {% for icon in payment_icons %}
                                                            {% assign icon_name = icon | strip %}
                                                            <div class="payment-card">
                                                                <img src="{{ icon_name | payment_type_img_url }}" alt="{{ icon_name }}" width="100" height="100">
                                                            </div>
                                                        {% endfor %}
                                                    {% endif %}
                                                </div>
                                            </div>
                                        {% endif %}
                                    </form>
                                
                                {% when 'testimonials' %}
                                    <div class="cart-box testimonial-cart-box" {{ block.shopify_attributes }}>
                                        <div dir="ltr" class="swiper tf-swiper" data-swiper='{
                                            "slidesPerView": 1,
                                            "spaceBetween": 12,
                                            "speed": 800,
                                            "pagination": { "el": ".sw-pagination-iconbox", "clickable": true },
                                            "navigation": {
                                                "clickable": true,
                                                "nextEl": ".nav-next-tes",
                                                "prevEl": ".nav-prev-tes"
                                            }
                                        }'>
                                            <div class="swiper-wrapper">
                                                {% if block.settings.testimonials != blank %}
                                                    {% assign testimonials = block.settings.testimonials | split: '||' %}
                                                    {% for testimonial in testimonials %}
                                                        {% assign testimonial_parts = testimonial | split: '|' %}
                                                        {% assign content = testimonial_parts[0] | strip %}
                                                        {% assign author_name = testimonial_parts[1] | strip %}
                                                        {% assign author_image = testimonial_parts[2] | strip %}
                                                        <div class="swiper-slide">
                                                            <div class="box-testimonial-main">
                                                                <span class="quote icon-quote5"></span>
                                                                <div class="content">
                                                                    <div class="list-star-default">
                                                                        {% for i in (1..5) %}
                                                                            <i class="icon-star"></i>
                                                                        {% endfor %}
                                                                    </div>
                                                                    <p class="text-review text-md text-main">{{ content | default: 'Stylish, comfortable, and perfect for any occasion! My new favorite fashion destination.' }}</p>
                                                                    <div class="box-author">
                                                                        <div class="img">
                                                                            {% if author_image != blank %}
                                                                                <img src="{{ author_image | img_url: '50x50', crop: 'center' }}" alt="{{ author_name }}" width="50" height="50">
                                                                            {% else %}
                                                                                <div class="avatar-placeholder" style="width: 50px; height: 50px; background: #f0f0f0; border-radius: 50%; display: flex; align-items: center; justify-content: center;">
                                                                                    <i class="icon icon-user" style="font-size: 24px; color: #999;"></i>
                                                                                </div>
                                                                            {% endif %}
                                                                        </div>
                                                                        <span class="name text-sm fw-medium">{{ author_name | default: 'Customer' }}</span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    {% endfor %}
                                                {% endif %}
                                            </div>
                                            <div class="box-nav-swiper">
                                                <div class="swiper-button-prev nav-swiper nav-prev-tes"></div>
                                                <div class="swiper-button-next nav-swiper nav-next-tes"></div>
                                            </div>
                                        </div>
                                    </div>
                            {% endcase %}
                        {% endfor %}
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
</div>



